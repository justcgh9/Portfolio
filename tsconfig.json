{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/*"]
        },
    "outDir": "dist"
  },
  "include": ["src/*.ts"],
}